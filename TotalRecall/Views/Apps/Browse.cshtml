@model List<TotalRecall.Models.TRApplication>
<table class="table table-striped table-responsive">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Public Key</th>
            <th>Created Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.PublicKey</td>
                <td>@item.LastUpdated</td>
            </tr>
        }
    </tbody>
</table>